         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    
    <servlet>
        <servlet-name>Tutorial4_16</servlet-name>
        <servlet-class>Tutorial4_16</servlet-class>
        <init-param>
            <param-name>number</param-name>
            <param-value>12345</param-value>
        </init-param>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Tutorial4_16</servlet-name>
        <url-pattern>/Tutorial4_16</url-pattern>
    </servlet-mapping>
    
      <servlet>
        <servlet-name>RegServlet</servlet-name>
